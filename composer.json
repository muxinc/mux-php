{
    "name": "muxinc/mux-php",
    "version": "0.0.1",
    "description": "Official PHP SDK for https://mux.com/video and https://mux.com/data",
    "keywords": [
        "php",
        "sdk",
        "rest",
        "api",
        "video",
        "streaming"
    ],
    "homepage": "https://mux.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Mux SDK team",
            "homepage": "https://mux.com",
            "email": "sdks@mux.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.12"
    },
    "autoload": {
        "psr-4": { "MuxPhp\\" : "MuxPhp/" }
    },
    "autoload-dev": {
        "psr-4": { "MuxPhp\\" : "test/" }
    }
}
